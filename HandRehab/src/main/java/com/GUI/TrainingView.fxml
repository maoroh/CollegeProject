<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="533.0" prefWidth="777.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.GUI.TrainingController">
   <children>
      <MenuBar layoutX="1.0" layoutY="1.0" prefHeight="29.0" prefWidth="777.0">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem mnemonicParsing="false" text="Close" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Edit">
            <items>
              <MenuItem mnemonicParsing="false" text="Delete" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <Button fx:id="button" layoutX="47.0" layoutY="408.0" mnemonicParsing="false" onAction="#buttonClicked" prefHeight="29.0" prefWidth="128.0" text="Show Exercise" />
      <ScrollPane layoutX="14.0" layoutY="67.0" prefHeight="222.0" prefWidth="295.0">
         <content>
            <TextArea fx:id="textArea" prefHeight="220.0" prefWidth="293.0" />
         </content>
      </ScrollPane>
      <Button fx:id="startRecordBtn" layoutX="240.0" layoutY="409.0" mnemonicParsing="false" onAction="#startRecord" text="Start Record" />
      <ImageView fx:id="exerciseImg" fitHeight="341.0" fitWidth="341.0" layoutX="351.0" layoutY="48.0" pickOnBounds="true" preserveRatio="true" />
   </children>
</AnchorPane>
